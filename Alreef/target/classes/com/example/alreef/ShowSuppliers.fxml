<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.alreef.ShowSuppliersController">
    <children>
        <TableView fx:id="table" layoutX="3.0" layoutY="14.0" prefHeight="400.0" prefWidth="305.0" AnchorPane.bottomAnchor="2.0">
            <columns>
                <TableColumn fx:id="ID" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="name" prefWidth="75.0" text="Name" />
                <TableColumn fx:id="address" prefWidth="75.0" text="Address" />
                <TableColumn fx:id="Phone" text="Phone" />
            </columns>
        </TableView>
        <Button fx:id="home" layoutX="330.0" layoutY="23.0" mnemonicParsing="false" onAction="#goHome" text="Home" />
    </children>
</AnchorPane>
