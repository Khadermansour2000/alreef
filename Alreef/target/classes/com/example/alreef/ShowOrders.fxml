<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.alreef.ShowOrderController">
    <children>
        <TableView fx:id="table" layoutX="268.0" prefHeight="400.0" prefWidth="348.0">
            <columns>
                <TableColumn fx:id="Onumber" prefWidth="93.0" text="Order number" />
                <TableColumn fx:id="ID" prefWidth="72.0" text="supplier ID" />
                <TableColumn fx:id="date" prefWidth="109.0" text="Date" />
                <TableColumn fx:id="total" prefWidth="72.0" text="Total" />
            </columns>
        </TableView>
        <Button fx:id="home" layoutX="14.0" layoutY="355.0" mnemonicParsing="false" onAction="#goHome" text="Home" />
        <Label layoutX="14.0" layoutY="26.0" prefHeight="17.0" prefWidth="65.0" text="Supplier ID :" />
        <TextField fx:id="inputid" layoutX="79.0" layoutY="22.0" promptText="ID" />
        <Button layoutX="79.0" layoutY="58.0" mnemonicParsing="false" onAction="#setFindId" text="find" />
        <Label layoutX="14.0" layoutY="106.0" prefHeight="17.0" prefWidth="65.0" text="date :" />
        <Button layoutX="79.0" layoutY="138.0" mnemonicParsing="false" onAction="#setFindDate" text="find" />
        <DatePicker fx:id="inputdate" layoutX="78.0" layoutY="102.0" />
    </children>
</AnchorPane>
