<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="370.0" prefWidth="502.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.alreef.CreateOrderController">
    <children>
        <Button fx:id="home" layoutX="76.0" layoutY="345.0" mnemonicParsing="false" onAction="#goHome" text="home" />
        <TableView fx:id="tabel" layoutX="11.0" layoutY="130.0" prefHeight="199.0" prefWidth="382.0">
            <columns>
                <TableColumn fx:id="ID" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="Mname" prefWidth="95.0" text="Name" />
                <TableColumn fx:id="price" prefWidth="75.0" text="Price" />
                <TableColumn fx:id="quantity" prefWidth="75.0" text="Quantity" />
                <TableColumn fx:id="total" prefWidth="75.0" text="Total" />
            </columns>
        </TableView>
        <Label layoutX="300.0" layoutY="337.0" prefHeight="17.0" prefWidth="35.0" text="total:">
            <font>
                <Font size="13.0" />
            </font></Label>
        <Label fx:id="Ototel" layoutX="334.0" layoutY="337.0" prefHeight="19.0" prefWidth="59.0" text="total" textAlignment="RIGHT">
            <font>
                <Font size="13.0" />
            </font></Label>
        <Label layoutX="14.0" layoutY="23.0" text="Supplier ID" />
        <Label layoutX="14.0" layoutY="59.0" text="Order date" />
        <DatePicker fx:id="Odate" layoutX="76.0" layoutY="55.0" onMouseClicked="#getname" />
        <Button fx:id="save" layoutX="12.0" layoutY="345.0" mnemonicParsing="false" text="save" />
        <TextField fx:id="Sid" layoutX="76.0" layoutY="19.0" onAction="#getname" />
        <Label fx:id="name" layoutX="236.0" layoutY="23.0" text="name :" />
        <Label fx:id="showname" layoutX="275.0" layoutY="23.0" prefHeight="17.0" prefWidth="85.0" />
        <Button fx:id="add" layoutX="17.0" layoutY="99.0" mnemonicParsing="false" onAction="#setAdd" prefHeight="25.0" prefWidth="58.0" text="Add" />
    </children>
</AnchorPane>
